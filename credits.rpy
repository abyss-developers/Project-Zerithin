label credits:
    $ credits_speed = 25
    play music "Tune_Leaps.mp3"
    scene black
    show fgsdgfsdg at Move((0.5, 1.0), (0.5, -1.78), credits_speed, xanchor=0.5, yanchor=0)
    with Pause(credits_speed+5)
    return
    
